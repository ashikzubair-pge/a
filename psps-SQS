Parameters:
  QueueName:
    Description: Name of queue to create
    Type: String
    MinLength: '1'
    MaxLength: '80'
    AllowedPattern: "[a-zA-Z0-9_-]+"
    ConstraintDescription: must be a valid queue name.
Resources:
  MyQueue:
    Type: AWS::SQS::Queue
    Properties:
      QueueName:
        Ref: QueueName

